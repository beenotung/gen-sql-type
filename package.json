{
  "name": "gen-sql-type",
  "version": "0.3.2",
  "description": "Generate Typescript Types from raw SQL statements",
  "keywords": [
    "code generation",
    "sql",
    "typescript",
    "type gen",
    "gen type"
  ],
  "author": {
    "name": "Beeno Tung",
    "email": "aabbcc1241@yahoo.com.hk",
    "url": "https://beeno-tung.surge.sh"
  },
  "license": "BSD-2-Clause",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "dev": "npm run bundle -- --watch",
    "bundle": "esbuild web/index.ts --outfile=web/bundle.js --bundle",
    "deploy": "npm run bundle && surge web",
    "prepare": "rimraf node_modules/@beenotung/tslib/index.*",
    "test": "run-s format tsc test:ts mocha",
    "clean": "rimraf dist",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "postformat": "tslint -p . --fix",
    "build": "run-s clean tsc",
    "tsc": "tsc -p tsconfig.build.json",
    "test:ts": "ts-node test/index.ts",
    "mocha": "ts-mocha \"{src,test}/**/*.spec.ts\"",
    "coverage": "nyc npm run mocha -- --reporter=progress",
    "report:update": "nyc --reporter=lcov npm run mocha -- --reporter=progress",
    "report:open": "open-cli coverage/lcov-report/index.html",
    "report": "run-s report:update report:open",
    "prepublishOnly": "run-s test build"
  },
  "husky": {
    "hook": {
      "pre-commit": "npm run format"
    }
  },
  "directories": {
    "test": "test"
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "@beenotung/tslib": "^16.41.1",
    "debug": "^4.3.1",
    "tslib": "^2.1.0"
  },
  "devDependencies": {
    "@types/chai": "^4.2.15",
    "@types/debug": "^4.1.5",
    "@types/mocha": "^8.2.1",
    "@types/node": "*",
    "@types/sinon": "^9.0.10",
    "chai": "^4.3.0",
    "esbuild": "^0.23.1",
    "husky": "^5.0.9",
    "mocha": "^8.3.0",
    "npm-run-all": "^4.1.5",
    "nyc": "^15.1.0",
    "open-cli": "^6.0.1",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2",
    "sinon": "^9.2.4",
    "surge": "^0.24.6",
    "ts-mocha": "^8.0.0",
    "ts-node": "^9.1.1",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "tslint-eslint-rules": "^5.4.0",
    "tslint-etc": "^1.13.9",
    "typescript": "^4.1.5"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/beenotung/gen-sql-type.git"
  },
  "bugs": {
    "url": "https://github.com/beenotung/gen-sql-type/issues"
  },
  "homepage": "https://github.com/beenotung/gen-sql-type#readme"
}
